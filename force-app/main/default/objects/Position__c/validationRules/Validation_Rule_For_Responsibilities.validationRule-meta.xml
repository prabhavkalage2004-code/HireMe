<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_Rule_For_Responsibilities</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT($Permission.Bypass_Validation),
      LEN( Responsibilities__c )   &gt; 100
)</errorConditionFormula>
    <errorMessage>Enter letter less than 100</errorMessage>
</ValidationRule>
