<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>first_last_Name</fullName>
    <active>true</active>
    <errorConditionFormula>OR( ISBLANK( First_Name__c ) ,ISBLANK(  Last_Name__c  ))</errorConditionFormula>
