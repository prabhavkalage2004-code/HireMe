<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_Number_validation_2</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( REGEX( Phone__c , &quot;[7-9]{1}[0-9]{9}&quot;) )</errorConditionFormula>
    <errorDisplayField>Phone__c</errorDisplayField>
    <errorMessage>Enter Correct Format of Number</errorMessage>
</ValidationRule>
